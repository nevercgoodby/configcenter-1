<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
    PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<settings>
		<!-- changes from the defaults -->
		<setting name="lazyLoadingEnabled" value="false" />
	</settings>
	
	<typeAliases>
		<typeAlias alias="hashMap" type="java.util.HashMap"/>
	</typeAliases>

	<plugins>
		<plugin interceptor="com.baidu.bjf.orm.mybatis.SqlMonitorManager">
			<property name="show_sql" value="true"/>
			<property name="callbackClass" value="com.baidu.fishingnet.bjf.orm.SqlEventMonitor"/>
		</plugin>
<!-- 		<plugin interceptor="com.baidu.bjf.security.mybatis.SecurityEnhanceInterceptor">
			<property name="securityId" value="29"/>
			<property name="securityServiceBeanName" value="securityClientLB"/>
		</plugin> -->		
	</plugins>
</configuration>
